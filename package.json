    "printer",
    "bluetooth",
    "net",
    "printer",
    "rn",
    "android"
  ],
  "dependencies": {
    "buffer": "^6.0.3",
    "iconv-lite": "^0.6.3",
    "react-xml-parser": "^1.1.8",
    "string_decoder": "^1.3.0"
  },
  "devDependencies": {
    "@tsconfig/react-native": "^3.0.5",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.3",
    "@types/react-test-renderer": "^18.3.0",
    "pod-install": "^0.1.10",
    "react": "^18.2.0",
    "react-native": "^0.74.2",
    "rimraf": "^3.0.2",
    "typescript": "~5.3.3"
  },
  "peerDependencies": {
    "react": "*",
